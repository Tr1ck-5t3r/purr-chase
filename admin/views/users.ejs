<h2>Manage Users</h2>
<table>
  <tr>
    <th>Username</th>
    <th>Email</th>
    <th>Actions</th>
  </tr>
  <% users.forEach(user => { %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.email %></td>
      <td>
        <a href="/delete-user/<%= user._id %>">Delete</a>
      </td>
    </tr>
  <% }) %>
</table>
